pipeline{
    agent any
    stages{

        stage("Compile"){
            steps{
                sh 'javac Test.java'
            }
          

        }
        stage("Run"){
            steps{
                sh 'java Test.java'
            }
            

        }
    }
    
    post{

        always{
            sh 'echo "always"'
        }
        success{
            sh 'echo "build success by aj"'
        }
        success{
            sh 'echo "build faliure by aj"'
        }
    }
}